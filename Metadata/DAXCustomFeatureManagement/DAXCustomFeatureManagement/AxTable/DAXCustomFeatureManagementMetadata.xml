		<Methods>
			<Method>
				<Name>findByFeatureState</Name>
				<Source><![CDATA[
    static DAXCustomFeatureManagementMetadata findByFeatureState(
        RefRecId _featureStateRecId,
        boolean _forUpdate = false,
        ConcurrencyModel _concurrencyModel = ConcurrencyModel::Auto)
    {
        DAXCustomFeatureManagementMetadata featureManagementMetadata;

        featureManagementMetadata.selectForUpdate(_forUpdate);

        if (_forUpdate && _concurrencyModel != ConcurrencyModel::Auto)
        {
            featureManagementMetadata.concurrencyModel(_concurrencyModel);
        }

        if (_featureStateRecId)
        {
            select firstonly * from featureManagementMetadata
                where featureManagementMetadata.FeatureState == _featureStateRecId;
        }

        return featureManagementMetadata;
    }

]]></Source>
