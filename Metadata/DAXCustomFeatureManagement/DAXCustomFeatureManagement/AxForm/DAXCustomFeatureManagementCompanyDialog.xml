			<Method>
				<Name>classDeclaration</Name>
				<Source><![CDATA[
[Form]
public class DAXCustomFeatureManagementCompanyDialog extends FormRun
{
    Set EnabledComapny;
    DAXCustomFeatureManagementState DAXCustomFeatureManagementState;
}
]]></Source>
			</Method>
			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    /// <summary>
    ///
    /// </summary>
    public void init()
    {
        DAXCustomFeatureManagementState = element.args().record();
        EnabledComapny = new Set(Types::String);
        
        super();

        DataArea    DataArea;
        while select DataArea
        {
            TmpCompanyLookup.DataArea = DataArea.id;
            TmpCompanyLookup.Name = DataArea.name;
            TmpCompanyLookup.insert();
        }

        DAXCustomFeatureManagementStateCompany  DAXCustomFeatureManagementStateCompany;
        while select crosscompany DAXCustomFeatureManagementStateCompany where
               DAXCustomFeatureManagementStateCompany.FeatureState == DAXCustomFeatureManagementState.RecId &&
                DAXCustomFeatureManagementStateCompany.IsEnabled
        {
            EnabledComapny.add(DAXCustomFeatureManagementStateCompany.DataAreaId);
        }
            
    }

]]></Source>
			</Method>
		</Methods>
		<DataSources xmlns="">
			<DataSource>
				<Name>TmpCompanyLookup</Name>
				<Methods>
					<Method>
						<Name>editMark</Name>
						<Source><![CDATA[
        public edit NoYes editMark(boolean _isSet,TmpCompanyLookup _companyLookup,NoYes _value)
        {
            if(_isSet)
            {
                if(_value)
                {
                    EnabledComapny.add(_companyLookup.DataArea);
                }
                else
                {
                    EnabledComapny.remove(_companyLookup.DataArea);
                }
            }
            return EnabledComapny.in(_companyLookup.DataArea);
        }

]]></Source>
					</Method>
				</Methods>
				<Fields />
			</DataSource>
		</DataSources>
		<DataControls xmlns="">
			<Control>
				<Name>OKCommandButton</Name>
				<Type>CommandButton</Type>
				<Methods>
					<Method>
						<Name>clicked</Name>
						<Source><![CDATA[
        /// <summary>
        ///
        /// </summary>
        public void clicked()
        {
            super();


            DAXCustomFeatureManagementStateCompany  DAXCustomFeatureManagementStateCompany;
            while select crosscompany DAXCustomFeatureManagementStateCompany where
               DAXCustomFeatureManagementStateCompany.FeatureState == DAXCustomFeatureManagementState.RecId &&
                DAXCustomFeatureManagementStateCompany.IsEnabled
            {
                if(!EnabledComapny.in(DAXCustomFeatureManagementStateCompany.DataAreaId))
                {
                    DAXCustomFeatureManagementStateCompany::disableFeature(DAXCustomFeatureManagementState.RecId,DAXCustomFeatureManagementStateCompany.DataAreaId);
                }
            }

            SetEnumerator se = EnabledComapny.getEnumerator();
            while(se.moveNext())
            {
                CompanyId companyId = se.current();
                DAXCustomFeatureManagementStateCompany::enableFeature(DAXCustomFeatureManagementState.RecId,companyId);
            }                    
        }

]]></Source>
					</Method>
				</Methods>
			</Control>
