						<Name>clicked</Name>
						<Source><![CDATA[
        /// <summary>
        ///
        /// </summary>
        public void clicked()
        {
            super();
            DAXCustomFeatureManagementState.openManagementCompanyDialog();
        }

]]></Source>
					</Method>
