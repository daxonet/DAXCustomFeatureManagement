		<Methods xmlns="">
			<Method>
				<Name>classDeclaration</Name>
				<Source><![CDATA[
[Form]
public class DAXCustomFeatureManagement extends FormRun
{
    NoYes byComapny;

}
]]></Source>
			</Method>
			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    /// <summary>
    ///
    /// </summary>
    public void init()
    {
        byComapny = element.args().parmEnum();
        super();

        if(byComapny)
        {
            ButtonCompany.visible(false);
            SysQuery::findOrCreateRange(DAXCustomFeatureManagementState_ds.queryBuildDataSource(),
                                        fieldNum(DAXCustomFeatureManagementState,IsEnabled))
                .value(queryValue(NoYes::Yes));

            SysQuery::findOrCreateRange(DAXCustomFeatureManagementMetadata_ds.queryBuildDataSource(),
                                        fieldNum(DAXCustomFeatureManagementMetadata,CompanyBase))
                .value(queryValue(NoYes::Yes));
            
            DAXCustomFeatureManagementState_ds.object(fieldNum(DAXCustomFeatureManagementState,IsEnabled)).visible(false);
            DAXCustomFeatureManagementMetadata_ds.object(fieldNum(DAXCustomFeatureManagementMetadata,EnableDate)).visible(false);
        }
        else
        {
            displayCompanyEnable.visible(false);
        }

        

    }

]]></Source>
			</Method>
		</Methods>
		<DataSources xmlns="">
			<DataSource>
				<Name>DAXCustomFeatureManagementMetadata</Name>
				<Methods>
					<Method>
						<Name>active</Name>
						<Source><![CDATA[
        /// <summary>
        ///
        /// </summary>
        /// <returns></returns>
        public int active()
        {
            int ret;
    
            ret = super();
            if(byComapny)
            {
                ButtonEnable.enabled(DAXCustomFeatureManagementStateCompany::find(DAXCustomFeatureManagementMetadata.FeatureState).IsEnabled == false);
                ButtonDisable.enabled(DAXCustomFeatureManagementStateCompany::find(DAXCustomFeatureManagementMetadata.FeatureState).IsEnabled);
            }
            else
            {
                ButtonEnable.enabled(DAXCustomFeatureManagementState.IsEnabled == false);
                ButtonDisable.enabled(DAXCustomFeatureManagementState.IsEnabled);
                ButtonCompany.enabled(DAXCustomFeatureManagementState.IsEnabled && DAXCustomFeatureManagementMetadata.CompanyBase);
            }
            return ret;
        }

]]></Source>
					</Method>
					<Method>
						<Name>displayCompanyEnable</Name>
						<Source><![CDATA[
        /// <summary>
        ///
        /// </summary>
        public display NoYes displayCompanyEnable(DAXCustomFeatureManagementMetadata _featureManagementMetadata)
        {
            return DAXCustomFeatureManagementStateCompany::find(_featureManagementMetadata.FeatureState).IsEnabled;
        }

]]></Source>
					</Method>
				</Methods>
				<Fields />
			</DataSource>
		</DataSources>
		<DataControls xmlns="">
			<Control>
				<Name>ButtonEnable</Name>
				<Type>Button</Type>
				<Methods>
					<Method>
						<Name>clicked</Name>
						<Source><![CDATA[
        /// <summary>
        ///
        /// </summary>
        public void clicked()
        {
            super();
            if(byComapny)
            {
                DAXCustomFeatureManagementStateCompany::enableFeature(DAXCustomFeatureManagementState.RecId);
            }
            else
            {
                DAXCustomFeatureManagementState::enableFeature(DAXCustomFeatureManagementState.RecId);
            }
            DAXCustomFeatureManagementMetadata_ds.executeQuery();
        }

]]></Source>
					</Method>
				</Methods>
			</Control>
			<Control>
