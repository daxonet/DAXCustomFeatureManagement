				<Type>Button</Type>
				<Methods>
					<Method>
						<Name>clicked</Name>
						<Source><![CDATA[
        /// <summary>
        ///
        /// </summary>
        public void clicked()
        {
            super();
            if(byComapny)
            {
                DAXCustomFeatureManagementStateCompany::disableFeature(DAXCustomFeatureManagementState.RecId);
            }
            else
            {
                DAXCustomFeatureManagementState::disableFeature(DAXCustomFeatureManagementState.RecId);
            }
            DAXCustomFeatureManagementMetadata_ds.executeQuery();
        }

]]></Source>
					</Method>
				</Methods>
			</Control>
